pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                echo 'Checking out code from Git...'
            }
        }
        stage('Build and Test') {
            steps {
                echo 'Building and testing the code...'
            }
        }
        stage('Deploy to Production') {
            steps {
                echo 'Deploying the code to production..'
            }
        }
    }
}
